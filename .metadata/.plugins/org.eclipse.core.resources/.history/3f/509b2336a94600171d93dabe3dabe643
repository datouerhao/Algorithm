
public class BinarySearch2 {
	public static int search(int searchKey) {
		int mid=0;
		int high=
		int low=0;
		while (true) {
			int mid = (high + low) / 2;
			if (a[mid] == searchKey) {
				return mid;
			}
			if (low >= high) {
				return -1;
			} else {
				if (searchKey < a[mid]) {
					return high = mid - 1;
				} else {
					return low = mid + 1;
				}
			}
		}

	}

	public static void main(String[] args) {
		int a[] = { 33, 22, 44, 11, 77, 55, 88 };
		System.out.println(search(88));
	}
}
