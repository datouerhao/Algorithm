
public class DoublyLinkedList {
	private Link first;
	private Link last;
	public DoublyLinkedList()
	{
		first=null;
		last=null;
	}
	public boolean isEmpty()
	{
		return first==null;
	}
	public void insertFirst(long dd)
	{
		Link newLink=new Link(dd);
		if(isEmpty())
			last=newLink;
		else
			first.previous=newLink;
		newLink.next=first;
		first=newLink;
		
	}
}
